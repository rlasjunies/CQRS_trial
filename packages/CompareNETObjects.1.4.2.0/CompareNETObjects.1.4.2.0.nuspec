    <id>CompareNETObjects</id>
    <version>1.4.2.0</version>
    <title>Compare .NET Objects</title>
    <authors>gfinzer</authors>
    <owners>gfinzer</owners>
    <licenseUrl>http://comparenetobjects.codeplex.com/license</licenseUrl>
